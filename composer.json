{
    "name": "yoypr/mia-eloquent-mezzio",
    "description": "The library for Mezzio",
    "version": "0.0.44",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [{
        "name": "Alejandro Rodriguez",
        "homepage": "https://yoypr.com"
    }],
    "require": {
        "php": ">=7.3",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0.1",
        "psr/http-server-middleware": "^1.0",
        "illuminate/database": "^8.20",
        "illuminate/pagination": "^8.20",
        "illuminate/filesystem": "^8.20",
        "yoypr/mia-core-mezzio": "v2.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Mia\\Database\\": "src/"
        }
    },
    "bin": [
        "bin/coda-migration"
    ],
    "scripts": {
        "coda-migrate": "coda-migration migrate",
        "coda-make-migrate": "coda-migration make",
        "coda-make-seeder": "coda-migration make:seeder",
        "coda-seed": "coda-migration db:seed"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/YOYPR-Inc/mia-core-mezzio.git"
        }
    ]
}
